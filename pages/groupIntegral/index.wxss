/* pages/groupIntegral/index.wxss */
.groupIntegral {
  /* position: fixed; */
  height: 100%;
  /* overflow: scroll; */
}

.header {
  position: relative;
  width: 100%;
  height: 360rpx;
  background: url('https://image.sqqmall.com/2020/07/06/1077339df1c2950c.png') no-repeat;
  background-size: 100% 100%;
  flex-wrap: wrap;
  align-content: flex-start;
  border-radius: 0px 0px 40rpx 40rpx;
}



.headerT {
  width: 100%;
  height: 62rpx;
  padding-top: 52rpx;
  padding-bottom: 26rpx;
}

.setBtn {
  position: absolute;
  display: inline-flex;
  width: 108rpx;
  right: 0;
  font-size: 20rpx;
  color: rgba(255, 255, 255, .8);
  flex-wrap: wrap;
  z-index: 1;
}

.setBtn navigator {
  display: inline-flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 42rpx;
  background: rgba(32, 140, 54, 1);
  border-radius: 200rpx 0px 0px 200rpx;
  margin-bottom: 20rpx;
}

.userInfo {
  position: relative;
  display: inline-flex;
  width: 100%;
  justify-content: center;
  flex-wrap: wrap;
}

.userInfo image {
  width: 82rpx;
  height: 82rpx;
  border-radius: 8rpx;
  margin-bottom: 14rpx;
  z-index: 1;
}

.userInfo::after {
  position: absolute;
  display: inline-block;
  content: '';
  width: 64rpx;
  height: 64rpx;
  background: #075E19;
  border-radius: 8px;
  opacity: 0.53;
  filter: blur(5px);
  top: 28rpx;
}

.userInfo span {
  width: 100%;
  text-align: center;
  color: white;
}

.userInfo .userName {
  font-size: 32rpx;
  margin-bottom: 10rpx;
  line-height: 32rpx;
}

.userInfo .groupName {
  font-size: 24rpx;
  margin-bottom: 18rpx;
}

.userInfo .userLine {
  display: inline-flex;
  justify-content: center;
}

.userInfo .userLine i:first-child {
  color: #FFBF46;
}

.userInfo .userLine .line {
  position: relative;
  display: inline-flex;
  width: 478rpx;
  align-items: center;
  margin: 0 20rpx 0 20rpx;
  border-radius: 4rpx;
}

.userInfo .userLine .line .nowLine {
  position: absolute;
  width: 134rpx;
  height: 8rpx;
  background: #FFBF46;
  border-radius: 10rpx;
  box-sizing: border-box;
  box-shadow: 0px 0px 8rpx 0px rgba(255, 191, 70, 1);
}

.userInfo .userLine .line .allLine {
  width: 100%;
  height: 8rpx;
  background: white;
  border: 1px solid rgba(255, 191, 70, 1);
  border-radius: 10rpx;
}

.userInfo .userLine i {
  font-size: 24rpx;
}

.userJy {
  font-size: 24rpx;
  color: #fff;
}

.userJy i:first-child {
  color: #FFBF46;
}

.groupBox {
  position: relative;
  display: flex;
  padding: 0 24rpx 0 24rpx;
  top: -60rpx;
  flex-wrap: wrap;
}

.groupBox .groupRouter {
  display: inline-flex;
  width: 100%;
  height: 152rpx;
  box-shadow: 0px 0px 8px 0px rgba(0, 0, 0, 0.12);
  border-radius: 8rpx;
  background: white;
  padding: 20rpx 80rpx 20rpx 80rpx;
  box-sizing: border-box;
  justify-content: space-between;
  margin-bottom: 32rpx;
}

.groupBox .groupRouter span {
  display: inline-flex;
  flex-wrap: wrap;
  width: 96rpx;
  justify-content: center;
}

.groupBox .groupRouter span image {
  width: 72rpx;
  height: 72rpx;
  box-shadow: 0px 0px 8px 0px rgba(0, 0, 0, 0.12);
  border-radius: 50%;
  margin-bottom: 6rpx;
}

.groupBox .groupRouter span i {
  font-size: 24rpx;
  color: #666;
}

.groupSearch {
  display: flex;
  width: 100%;
  height: 52rpx;
  margin-bottom: 24rpx;
}

.groupSearch span {
  display: inline-flex;
  align-items: center;
}

.groupSearch .reSet {
  display: inline-flex;
  justify-content: center;
  align-items: center;
  width: 144rpx;
  height: 100%;
  border-radius: 28rpx;
  border: 2rpx solid rgba(73, 166, 92, 1);
  box-sizing: border-box;
  font-size: 24rpx;
  color: #49A65C;
  margin-right: 12rpx;
}

.groupSearch .reInput {
  flex: 1;
  background: #F6F6F6;
  border-radius: 28rpx;
}

.groupSearch .reInput image {
  width: 28rpx;
  height: 28rpx;
  margin: 0 12rpx 0 24rpx;
}

.groupSearch .reInput input {
  font-size: 28rpx;
}

.groupList {
  display: flex;
  width: 100%;
}

.groupList ul {
  display: inline-flex;
  width: 100%;
  padding: 0 24rpx 0 24rpx;
  flex-wrap: wrap;
  align-content: flex-start;
  margin-bottom: 40rpx;
  box-shadow: 0px 2rpx 4rpx 0px rgba(0, 0, 0, 0.06);
  border-radius: 8rpx;
}

.groupList ul .listText {
  display: inline-flex;
  height: 78rpx;
  align-items: center;
  font-size: 24rpx;
  color: #999999;
  justify-content: space-between;
  border-bottom: 2rpx solid #EEEEEE;
  box-sizing: border-box;
  margin-bottom: 24rpx;
}

.groupList ul .listText:first-child {
  justify-content: flex-start;
}

.groupList ul .listText:first-child text:first-child {
  margin-right: 48rpx;
}

.groupList ul .listText:first-child text:last-child {
  padding-right: 194rpx;
}
.groupList ul .listTextR text:last-child{
  margin-right: 12rpx;
}
.groupList ul li {
  position: relative;
  display: inline-flex;
  width: 100%;
  height: 60rpx;
  margin-bottom: 40rpx;
}

.groupList ul li span {
  display: inline-flex;
  height: 100%;
  align-items: center;
}

.groupList ul li span u image {
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  margin-right: 12rpx;
}

.groupList ul li .rank {
  width: 40rpx;
  margin-right: 56rpx;
}

.groupList ul li .info {
  width: 192rpx;
  margin-right: 50rpx;
}

.groupList ul li .info u {
  display: inline-flex;
  flex-wrap: wrap;
}

.groupList ul li .info u i:first-child {
  width: 120rpx;
  font-size: 28rpx;
  color: #333;
  line-height: 28rpx;
  margin-bottom: 4rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.groupList ul li .info u i:last-child {
  font-size: 20rpx;
  width: 56rpx;
  height: 28rpx;
  text-align: center;
  line-height: 28rpx;
  background: linear-gradient(135deg, rgba(105, 212, 121, 1) 0%, rgba(73, 166, 92, 1) 100%);
  color: white;
  border-radius: 8rpx;
}

.istube {
  background: linear-gradient(133deg, rgba(255, 225, 128, 1) 0%, rgba(255, 192, 73, 1) 100%) !important;
}

.groupList ul li .name {
  display: block;
  width: 160rpx;
  line-height: 60rpx;
  font-size: 24rpx;
  color: #333;
  margin-right: 48rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}

.groupList ul li .number {
  width: 60rpx;
  color: #333;
  font-size: 24rpx;
  margin-right: 24rpx;
}

.groupList ul li .set {
  position: absolute;
  right: 0;
}

.groupList ul li .set image {
  width: 24rpx;
  height: 24rpx;
}

.backBtn {
  display: flex;
  align-items: center;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 98rpx;
  background: white;
}

.backBtn .bottons {
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  margin: 0 24rpx 0 24rpx;
  background: linear-gradient(135deg, rgba(105, 212, 121, 1) 0%, rgba(73, 166, 92, 1) 100%);
  border-radius: 44rpx;
  font-size: 32rpx;
  color: white;
  font-weight: normal;
}

.jfBox {
  display: flex;
  height: 100%;
  padding: 0 84rpx 0 84rpx;
  justify-content: center;
  align-items: center;
}

.jfBox .jfBoxInfo {
  position: relative;
  display: inline-flex;
  width: 100%;
  background: white;
  border-radius: 8rpx;
  flex-wrap: wrap;
  align-content: flex-start;
}

.closeImg {
  position: absolute;
  width: 48rpx;
  height: 48rpx;
  right: -50rpx;
  top: -74rpx;
}

.jfBox .jfBoxInfo p {
  display: inline-flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 98rpx;
  background: #49A65C;
  border-radius: 8rpx 8rpx 0px 0px;
  font-size: 32rpx;
  color: white;
  margin-bottom: 36rpx;
}

.jfBox .jfBoxInfo ul {
  display: inline-flex;
  width: 100%;
  flex-wrap: wrap;
  padding: 0 30rpx 30rpx 30rpx;
}

.jfBox .jfBoxInfo ul li {
  display: inline-flex;
  width: 100%;
  height: 64rpx;
  justify-content: space-between;
  margin-bottom: 42rpx;
}

.jfBox .jfBoxInfo ul li:last-child {
  margin-bottom: 0;
}

.jfBox .jfBoxInfo ul li span {
  display: inline-flex;
  align-items: center;
}

.jfBox .jfBoxInfo ul li span image {
  width: 64rpx;
  height: 64rpx;
  margin-right: 20rpx;
}

.jfBox .jfBoxInfo ul li span i {
  display: inline-flex;
  width: 224rpx;
  height: 100%;
  align-content: space-between;
  flex-wrap: wrap;
  font-size: 24rpx;
  line-height: 24rpx;
  color: #999999;
}

.jfBox .jfBoxInfo ul li span i u:first-child {
  font-size: 28rpx;
  line-height: 28rpx;
  color: #333333;
}

.jfBox .jfBoxInfo ul li span:last-child {
  color: #49A65C;
  font-size: 28rpx;
}