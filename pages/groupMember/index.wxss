/* pages/groupMember/index.wxss */
.groupMember {
  position: fixed;
  width: 100%;
  height: 100%;
  overflow: scroll;
}
.searchBox{
  display: inline-flex;
  width: 100%;
  align-items: center;
  padding: 0 24rpx 20rpx 24rpx;
  box-sizing: border-box;
  border-bottom: 12rpx solid #F6F6F6;
  
}
.searchBox span{
  display: inline-flex;
  width: 96rpx;
  height: 52rpx;
  margin-right: 12rpx;
  border: 2rpx solid #49A65C;
  box-sizing:border-box;
  border-radius: 28rpx;
  font-size: 24rpx;
  color: #49A65C;
  align-items: center;
  justify-content: center;
}

.searchBox .search {
  display: inline-flex;
  width: 100%;
  height: 56rpx;
  background: #F6F6F6;
  border-radius: 28rpx;
  padding: 0 24rpx 0 24rpx;
  align-items: center;
}

.searchBox .search image {
  width: 28rpx;
  height: 28rpx;
  margin-right: 12rpx;
}

.searchBox .search input {
  color: #333;
  font-size: 28rpx;
  flex: 1;
}

.infoList ul {
  display: flex;
  box-sizing: border-box;
  flex-wrap: wrap;
  background: white;
  margin: 0 24rpx 0 24rpx;
  border-radius: 8rpx;
}

.infoList ul .dataType {
  display: inline-flex;
  width: 100%;
  height: 78rpx;
  align-items: center;
  justify-content: space-between;
  color: #999;
  font-size: 24rpx;
  border-bottom: 2rpx solid #EEEEEE;
  box-sizing: border-box;
  margin-bottom: 24rpx;
}

.infoList ul .dataType view:first-child text:first-child {
  margin-right: 210rpx;
}
.infoList ul .dataType view:last-child text:first-child {
  margin-right: 108rpx;
}
.infoList ul .dataLine {
  display: inline-flex;
  align-content: flex-start;
  width: 100%;
  max-height: 900rpx;
  flex-wrap: wrap;
  overflow-y: scroll;
}

.infoList ul .dataLine li {
  display: inline-flex;
  width: 100%;
  height: 40rpx;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 36rpx;
}

.infoList ul .dataLine li view {
  display: inline-flex;
  align-items: center;
}

.infoList ul .dataLine li image {
  width: 36rpx;
  height: 36rpx;
  border-radius: 50%;
  margin-right: 12rpx;
}

.infoList ul .dataLine li .dataName {
  width: 140rpx;
  font-size: 24rpx;
  color: #333;
  margin-right: 12rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.infoList ul .dataLine li .identity{
  width: 56rpx;
  height: 28rpx;
  text-align: center;
  line-height: 28rpx;
  border-radius: 4rpx;
  font-size: 20rpx;
  color: white;
  margin-right: 26rpx;
}
.qz{
  background:linear-gradient(135deg,rgba(105,212,121,1) 0%,rgba(73,166,92,1) 100%);
}
.gl{
  background:linear-gradient(133deg,rgba(255,225,128,1) 0%,rgba(255,192,73,1) 100%); 
}

.infoList ul .dataLine li .dataTime{
  width: 140rpx;
  font-size: 24rpx;
  margin-right: 26rpx;
}

.infoList ul .dataLine li .dataBtn {
  width: 140rpx;
  font-size: 24rpx;
  color: #333333;
}

.infoList ul .dataLine li .dataLook {
  display: inline-flex;
  align-items: center;
  justify-content: center;
  padding: 0 8rpx 0 8rpx;
  height: 40rpx;
  color: #999999;
  font-size: 20rpx;
  border: 2rpx solid #999999;
  box-sizing: border-box;
  border-radius: 8rpx;
}
.setGl{
  border-color: #FFBF46 !important;
  color: #FFBF46 !important;
}



.floatMenu{
  position: absolute;
  display: flex;
  padding: 24rpx 50rpx 24rpx 50rpx;
  width: 260rpx;
  height: 526rpx;
  background: white;
  border-radius: 8rpx;
  z-index: 1;
  top:92rpx;
  box-shadow:0px 4rpx 8rpx 0px rgba(1,110,24,0.12);
  box-sizing: border-box;
  flex-wrap: wrap;
  align-content: flex-start;
}
.floatMenu li{
  display: inline-flex;
  width: 100%;
  height: 40rpx;
  justify-content: flex-start;
  align-items: center;
  margin-bottom: 48rpx;
}
.floatMenu li image{
  width: 36rpx;
  height: 36rpx;
  margin-right: 12rpx;
}
.floatMenu li i{
  color: #666660;
  font-size: 28rpx;
}


.backBtn {
  position: fixed;
  display: flex;
  width: 100%;
  align-items: center;
  bottom: 40rpx;
  left: 0;
}

.backBtn .bottons {
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  margin: 0 24rpx 0 24rpx;
  background: linear-gradient(135deg, rgba(105, 212, 121, 1) 0%, rgba(73, 166, 92, 1) 100%);
  border-radius: 44rpx;
  font-size: 32rpx;
  color: white;
}