/* pages/myPrizeList/index.wxss */
.prizeList {
  position: fixed;
  width: 100%;
  height: 100%;
  background: #F6F6F6;
  overflow-y: scroll;
  padding: 24rpx;
  box-sizing: border-box;
}

.explain {
  padding: 24rpx;
  font-size: 24rpx;
  color: #333;
  background: white;
  border-radius: 8rpx;
  line-height: 34rpx;
  box-shadow: 0px 2rpx 4rpx 0px rgba(0, 0, 0, 0.06);
  margin-top: 127rpx;
}
.myIpx{
  margin-top: 179rpx;
}
.title {
  display: inline-flex;
  width: 100%;
  height: 94rpx;
  align-items: center;
  justify-content: space-between;
  font-size: 32rpx;
  color: #333;
}

.title i:last-child {
  font-size: 28rpx;
  color: #999;
}

.title i:last-child u {
  color: #49A65C;
}

.prizeList ul {
  display: inline-flex;
  width: 100%;
  flex-wrap: wrap;
  margin-bottom: 80rpx;
}

.prizeList ul li {
  display: inline-flex;
  flex-wrap: wrap;
  width: 100%;
  height: 160rpx;
  background: #EBF1ED;
  align-items: center;
  padding: 0 24rpx 0 24rpx;
  box-shadow: 0px 2rpx 4rpx 0px rgba(0, 0, 0, 0.06);
  border-radius: 14rpx;
  margin-bottom: 20rpx;
}

.prizeList ul li span {
  display: inline-flex;
  width: 100%;
}

.prizeList ul li span:first-child {
  justify-content: space-between;
  /* padding-bottom: 20rpx; */
  /* border-bottom: 2rpx dashed #F1F1F1; */
  box-sizing: border-box;
}

.prizeList ul li p {
  display: inline-flex;
  align-items: center;
}

.prizeList ul li p image {
  width: 110rpx;
  height: 110rpx;
  margin-right: 10rpx;
  border-radius: 4rpx;
}

.prizeList ul li p:first-child dd {
  width: 326rpx;
}
.prizeList ul li p:last-child{
  border-left: 2rpx dashed #49A65C;
  box-sizing: border-box;
  padding:  36rpx 0 36rpx 24rpx;
}
.prizeList ul li p:last-child b {
  display: inline-flex;
  width: 160rpx;
  height: 50rpx;
  align-items: center;
  justify-content: center;
  background: #49A65C;
  border-radius: 29rpx;
  font-size: 28rpx;
  color: #fff;
}

.prizeList ul li p dd {
  display: inline-flex;
  flex-wrap: wrap;
  width: 100%;
  height: 100%;
  font-size: 32rpx;
  color: #333;
  align-content: flex-start;
}
.prizeList ul li p dd u{
  font-size: 24rpx;
  color: #999999;
  margin-top: 12rpx;
}
.prizeList ul li p dd u em{
  color: #333333;
}
.backBtn {
  display: flex;
  align-items: center;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 98rpx;
  background: white;
  z-index: 1;
  box-shadow: 0px -2px 8px 0px rgba(0, 0, 0, 0.06);

}

.backBtn .bottons {
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  margin: 0 24rpx 0 24rpx;
  background: linear-gradient(135deg, rgba(105, 212, 121, 1) 0%, rgba(73, 166, 92, 1) 100%);
  border-radius: 44rpx;
  font-size: 32rpx;
  color: white;
  font-weight: normal;
}