div{
  display: block;
}
.flex {
  display: flex;
  align-items: center;
}

.flex-1 {
  flex: 1;
}

button {
  background: none;
  font-size: inherit;
  color: inherit;
  line-height: 1;
  text-align: center;
}

.navigator-hover {
  background: inherit;
  color: inherit;
  opacity: .7;
}

.btn-hover {
  opacity: .7;
}


.turntable {
  position: relative;
  background: #fb364a url("https://image.sqqmall.com/2020/07/07/3430d7c30421b8fa.png") center bottom no-repeat;
  background-size: 100% auto;
  overflow: hidden;
}
.header{
  position: relative;
}

.header::after {
  display: inline-block;
  content: "";
  position: relative;
  left: 50%;
  margin-left: -135rpx;
  width:270rpx;
  height: 190rpx;
  background: url("https://image.sqqmall.com/2020/07/07/a94529733a81641b.png") center top no-repeat;
  background-size: 100% auto;
  z-index: 1;
}

.header .my{
  position: absolute;
  width: 200rpx;
  height: 88rpx;
  left: 20rpx;
  top:30rpx
}
.header .my image{
  width: 100%;
  height: 100%;
}

.header .ls{
  display: inline-flex;
  width: 150rpx;
  flex-wrap: wrap;
  position: absolute;
  right: 20rpx;
  top: 30rpx;
}

.header .ls navigator{
  display: inline-flex;
  width: 100%;
  height: 40rpx;
  font-size: 22rpx;
  background: rgb(255, 186, 45);
  border-radius: 40rpx;
  justify-content: center;
  align-items: center;
  color: #ba271a;
  margin-bottom: 20rpx;
}


.turntable .s1 {
  position: relative;
}

.turntable .s1:after {
  content: "";
  position: absolute;
  bottom: -30rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 440rpx;
  height: 60rpx;
  border-radius: 50%;
  background: rgba(0, 0, 0, 0.15);
}

.turntable .s1 .main {
  position: relative;
  margin: 20rpx auto 60rpx auto;
  width: 560rpx;
  height: 560rpx;
  border-radius: 50%;
  overflow: hidden;
  background: #ffbf27;
  z-index: 2;
}

.turntable .s1 .item-box {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: #f2c8ad;
}

.turntable .s1 .item-box:after {
  content: "";
  position: absolute;
  top: -2%;
  left: -2%;
  width: 104%;
  height: 104%;
  background: url("https://image.sqqmall.com/2020/07/07/7b9d69211f6a66c0.png") center no-repeat;
  background-size: 100% 100%;
  z-index: 5;
}

.turntable .s1 .item {
  display: inline-flex;
  flex-wrap: wrap;
  position: absolute;
  top: -20%;
  left: 15%;
  width: 70%;
  height: 70%;
  padding-top: 80rpx;
  justify-content: center;
  align-content: flex-end;
  transform-origin: center bottom;
  box-sizing: border-box;
  /* clip-path: polygon(50% 100%, 8% 0, 92% 0); */
  clip-path: polygon(-50% 0, 150% 0, 50% 100%)
}

.turntable .s1 .item:nth-child(odd) {
  background: #fff;
}

.turntable .s1 .item .text {
  margin: auto;
  color: #ec4313;
  font-size: 24rpx;
  width: 100%;
  line-height: 1.2;
  text-align: center;
}

.turntable .s1 .item .img {
  margin: 10rpx auto 0 auto;
  display: block;
  width: 60rpx;
  height: 60rpx;
  margin-bottom: 90rpx;
}



.turntable .s1 .item:nth-child(1) {
  transform: rotate(0deg);
}

.turntable .s1 .item:nth-child(2) {
  transform: rotate(90deg);
}

.turntable .s1 .item:nth-child(3) {
  transform: rotate(180deg);
}

.turntable .s1 .item:nth-child(4) {
  transform: rotate(270deg);
}

