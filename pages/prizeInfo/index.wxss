/* pages/myPrizeList/index.wxss */
.prizeList {
  position: fixed;
  width: 100%;
  height: 100%;
  overflow: scroll;
  background: #F6F6F6;
  padding: 24rpx;
  box-sizing: border-box;
}

.explain {
  padding: 24rpx;
  font-size: 24rpx;
  color: #E84855;
  background: white;
  border-radius: 8rpx;
  line-height: 34rpx;
  box-shadow: 0px 2rpx 4rpx 0px rgba(0, 0, 0, 0.06);
}

.title {
  display: inline-flex;
  width: 100%;
  height: 94rpx;
  align-items: center;
  justify-content: space-between;
  font-size: 32rpx;
  color: #333;
}

.infoList .dataType {
  display: inline-flex;
  width: 100%;
  height: 78rpx;
  align-items: center;
  justify-content: space-between;
  color: #999;
  font-size: 24rpx;
  border-bottom: 2rpx solid #EEEEEE;
  box-sizing: border-box;
  margin-bottom: 24rpx;
}

.dataBox{
  background: white;
  box-shadow:0px 2rpx 4px 0rpx rgba(0,0,0,0.06);
  border-radius:8rpx;
}
.dataType {
  display: inline-flex;
  width: 100%;
  height: 78rpx;
  align-items: center;
  justify-content: space-between;
  color: #999;
  font-size: 24rpx;
  border-bottom: 2rpx solid #EEEEEE;
  box-sizing: border-box;
  margin-bottom: 24rpx;
  padding: 0 24rpx 0 24rpx;
}
.dataType .datatit{
  display: inline-flex;
  flex: 1;
}
.dataType .datatit:last-child{
  justify-content: space-between;
}
.dataType view:first-child text:first-child {
  display: inline-block;
  width: 160rpx;
  margin-right: 44rpx;
}
.dataType view:last-child text:first-child {
  margin-left: 60rpx;
}
.dataLine {
  display: inline-flex;
  width: 100%;
  max-height: 800rpx;
  flex-wrap: wrap;
  overflow-y: scroll;
  padding: 0 24rpx 0 24rpx;
  box-sizing: border-box;
  align-content: flex-start;
}

.dataLine li {
  display: inline-flex;
  width: 100%;
  height: 40rpx;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 40rpx;
}

.dataLine li view {
  display: inline-flex;
  align-items: center;
}

.dataLine li .dataName {
  width: 160rpx;
  font-size: 28rpx;
  color: #333;
  margin-right: 44rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.dataLine li .dataTime{
  width: 132rpx;
  font-size: 24rpx;
  margin-right: 42rpx;
}

.dataLine li .dataBtn {
  font-size: 20rpx;
}

.dataLine li .dataBtn i {
  padding: 0 12rpx 0 12rpx;
  height: 40rpx;
  box-sizing: border-box;
  border-radius: 8rpx;
  color: #999999;
  font-size: 24rpx;
}

.dataLine li .dataBtn .send {
  border: 2rpx solid #FFBF46;
  color: #FFBF46;
  border-radius: 8rpx;
  margin-right: 12rpx;
}

.dataLine li .dataBtn .over {
  border: 2rpx solid#E84855;
  color: #E84855;
}

.dataLine li .dataLook {
  font-size: 24rpx;
  color: #49A65C;
  text-decoration: underline;
}
