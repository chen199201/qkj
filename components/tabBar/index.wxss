/* components/tabBar/index.wxss */
.tabBar{
  position: fixed;
  display: flex;
  left: 0;
  bottom: 0;
  box-sizing: border-box;
  width: 100%;
  height: 100rpx;
  justify-content: space-between;
  background: white;
  padding: 10rpx 46rpx 6rpx 46rpx ;
  box-shadow:0px -2rpx 8rpx 0px rgba(0,0,0,0.06);
}
.tabBar li{
  display: inline-flex;
  width: 96rpx;
  flex-wrap: wrap;
  justify-content: center;
  align-content: center;
}
.tabBar li image{
  width: 56rpx;
  height: 56rpx;
}
.tabBar li text{
  width: 100%;
  font-size: 20rpx;
  color: #8E8E93;
  text-align: center;
}
.tabBar li .inRouter{
  color: #49A65C;
}